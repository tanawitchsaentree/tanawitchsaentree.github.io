{
    "version": "1.0",
    "system_name": "Entity Extraction Patterns",
    "entities": {
        "company_name": {
            "type": "organization",
            "patterns": [
                "(?:at|worked at|experience at|time at|from|with|for)\\s+([A-Z][a-zA-Z]*(?:\\s+[A-Z][a-zA-Z]*)*)",
                "(Allianz(?:\\s+Technology)?(?:\\s+Thailand)?)",
                "(Invitrace(?:\\s+Health)?)",
                "(CP\\s+Origin)",
                "(DoctorAnywhere)",
                "(Codefin)",
                "(Peakaccount)"
            ],
            "known_entities": [
                "Allianz Technology Thailand",
                "Allianz",
                "Invitrace Health",
                "Invitrace",
                "CP Origin",
                "DoctorAnywhere",
                "Codefin",
                "Peakaccount"
            ],
            "aliases": {
                "invitrace health": "Invitrace",
                "doctor anywhere": "DoctorAnywhere",
                "cp": "CP Origin"
            }
        },
        "skill_name": {
            "type": "skill",
            "patterns": [
                "(?:good at|skilled in|knows|experienced in|expert in|proficient in)\\s+([a-zA-Z\\s]+?)(?:\\?|$|and|or)",
                "(?:with|using)\\s+([A-Z][a-zA-Z]+)"
            ],
            "known_entities": [
                "Design Systems",
                "User Research",
                "UX Strategy",
                "Visual Design",
                "Interaction Design",
                "Prototyping",
                "Figma",
                "Design Leadership",
                "Information Architecture",
                "Accessibility",
                "Usability Testing"
            ],
            "fuzzy_match": true,
            "threshold": 0.8
        },
        "time_period": {
            "type": "temporal",
            "patterns": [
                "(\\d{4})",
                "(recent|current|latest|past|previous)",
                "(last\\s+\\d+\\s+years?)",
                "(in\\s+\\d{4})",
                "(since\\s+\\d{4})",
                "(from\\s+\\d{4}\\s+to\\s+\\d{4})"
            ],
            "known_entities": [
                "2025",
                "2024",
                "2023",
                "2022",
                "2021",
                "2020",
                "recent",
                "current",
                "latest"
            ],
            "resolve_relative": true
        },
        "role_title": {
            "type": "job_title",
            "patterns": [
                "(?:as|was|is)\\s+(?:a\\s+)?([A-Z][a-zA-Z]+(?:\\s+[A-Z][a-zA-Z]+){1,3})",
                "(Lead\\s+Product\\s+Designer)",
                "(Senior\\s+(?:Product\\s+)?Designer)",
                "(UX/UI\\s+Designer)",
                "(Product\\s+Designer)"
            ],
            "known_entities": [
                "Senior Designer",
                "Lead Product Designer",
                "Senior Product Designer",
                "Senior UX/UI Designer",
                "Senior Designer",
                "UX/UI Designer"
            ]
        },
        "contact_method": {
            "type": "communication",
            "patterns": [
                "(email|linkedin|github|behance|medium)",
                "(e-?mail)",
                "(social\\s+media)"
            ],
            "known_entities": [
                "email",
                "linkedin",
                "github",
                "behance",
                "medium"
            ]
        },
        "industry": {
            "type": "sector",
            "patterns": [
                "(healthcare|health\\s+tech|healthtech)",
                "(finance|fintech|financial)",
                "(insurance|insurtech)",
                "(productivity|productivity\\s+tools)"
            ],
            "known_entities": [
                "healthcare",
                "healthtech",
                "fintech",
                "finance",
                "insurance tech",
                "productivity"
            ],
            "aliases": {
                "health tech": "healthtech",
                "financial tech": "fintech",
                "insurance tech": "insurtech"
            }
        }
    },
    "extraction_rules": {
        "company_name": {
            "priority": 1,
            "validate": "known_entities_first",
            "case_sensitive": false,
            "max_extractions": 3
        },
        "skill_name": {
            "priority": 2,
            "validate": "fuzzy_match",
            "case_sensitive": false,
            "max_extractions": 5
        },
        "time_period": {
            "priority": 3,
            "validate": "pattern_match",
            "normalize": true,
            "max_extractions": 2
        },
        "role_title": {
            "priority": 2,
            "validate": "known_entities_first",
            "case_sensitive": false,
            "max_extractions": 2
        }
    },
    "entity_relationships": {
        "company_name + role_title": {
            "type": "employment",
            "query_pattern": "experience_at_company"
        },
        "skill_name + company_name": {
            "type": "skill_demonstration",
            "query_pattern": "skill_used_at_company"
        },
        "company_name + time_period": {
            "type": "temporal_employment",
            "query_pattern": "work_during_period"
        }
    }
}