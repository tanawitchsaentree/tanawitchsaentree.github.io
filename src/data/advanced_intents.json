                    "experience_query",
                    "contact_query",
                    "quick_summary"
                ],
                "boost_factor": 1.5
            },
            "peer_designer": {
                "boost_intents": [
                    "skills_query",
                    "company_specific",
                    "deep_dive"
                ],
                "boost_factor": 1.3
            },
            "student": {
                "boost_intents": [
                    "skills_query",
                    "surprise_query"
                ],
                "boost_factor": 1.2
            }
        },
        "conversation_flow": {
            "greeting": {
                "likely_next": [
                    "experience_query",
                    "skills_query",
                    "quick_summary"
                ]
            },
            "experience_query": {
                "likely_next": [
                    "skills_query",
                    "company_specific",
                    "contact_query"
                ]
            },
            "skills_query": {
                "likely_next": [
                    "experience_query",
                    "skill_specific",
                    "contact_query"
                ]
            }
        }
